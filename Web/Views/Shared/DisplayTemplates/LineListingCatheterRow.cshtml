@model IQI.Intuition.Web.Models.Reporting.Catheter.Facility.LineListingCatheterView.CatheterRow

<tr>
    <td style="vertical-align:top;width:200px;">
        <div>
        <div style="font-weight:bold;">Started:</div> @Model.StartedOn
        </div>
        <div>
        <div style="font-weight:bold;">Discontinued:</div> @Model.DiscontinuedOn
        </div>
    </td>
    <td style="vertical-align:top;width:300px;">
        <div style="color:Black;font-weight:bold;">@Model.PatientName</div>
        <div><span style="font-weight:bold;">Birthdate:</span> @Model.PatientBirthDate</div>
        <div><span style="font-weight:bold;">Admitted:</span> @Model.AdmissionDate</div>
        <hr />
        <div>Current Location</div>
        <div><span style="font-weight:bold;">Room:</span> @Model.Room</div>
        <div><span style="font-weight:bold;">Wing:</span> @Model.Wing</div>
        <div><span style="font-weight:bold;">Floor:</span> @Model.Floor</div>
        <hr />
        <div>Catheter Initial Location</div>
        <div><span style="font-weight:bold;">Room:</span> @Model.InitialRoom</div>
        <div><span style="font-weight:bold;">Wing:</span> @Model.InitialWing</div>
        <div><span style="font-weight:bold;">Floor:</span> @Model.InitialFloor</div>

    </td>
    <td style="vertical-align:top;width:300px;">
        @if (Convert.ToBoolean(ViewData["showLink"]))
        {
            <div style="padding-bottom:5px;font-weight:bold;">
            <a href="@Url.Action("Edit", new { Controller = "Catheter", Area = "", id = Model.ID, returnurl = Request.Url.AbsoluteUri })">
            Catheter</a>
            </div>
        }
        <div style="font-weight:bold;">Diagnosis</div>
        <div>@Model.Diagnosis</div>
                    
    </td>
                
</tr>
