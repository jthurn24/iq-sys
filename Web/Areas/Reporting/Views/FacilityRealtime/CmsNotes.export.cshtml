@model IQI.Intuition.Web.Models.Reporting.CmsMatrix.NotesView
           
@{
    ViewBag.Layout_ContentTitle = "CMS Notes";
}


@foreach (var note in Model.Entries)
{
    <div style="page-break-inside:avoid;border-top:1px solid black;padding:10px;">
        <div style="font-weight:bold;font-size:16px;padding-bottom:10px;">@note.Name</div>
        <div>@Html.Raw(note.NoteText)</div>
    </div>
}


<div style="clear:both;"></div>